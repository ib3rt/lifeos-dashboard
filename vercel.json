{
  "version": 2,
  "name": "lifeos-dashboard",
  "builds": [
    {
      "src": "**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/docs-viewer/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ]
}
